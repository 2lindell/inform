packagetype _plain
packagetype _code
packagetype _linkage

package main _plain
	kind K_number int32
	constant C_alpha K_number = 1
	package connectors _linkage
		plug faraway ~~> "timbuktu"
		plug distant ~~> "ultima_thule"
		socket gamma ~~> /main/sub/subsub/C_gamma
	package sub _plain
		constant C_beta /main/K_number = /main/C_alpha
		package subsub _plain
			constant C_gamma /main/K_number = /main/C_alpha
			constant C_delta /main/K_number = /main/sub/C_beta
		constant C_epsilon /main/K_number = /main/sub/subsub/C_gamma
