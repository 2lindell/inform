packagetype _plain
packagetype _code
packagetype _linkage

package main _plain
	kind K_number int32
	constant (K_number) C_alpha = 1
	package connectors _linkage
		plug faraway ~~> "timbuktu"
		plug distant ~~> "ultima_thule"
		socket gamma ~~> /main/sub/subsub/C_gamma
